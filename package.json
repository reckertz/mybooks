  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reckertz/mybooks/issues"
  },
  "homepage": "https://github.com/reckertz/mybooks#readme",
  "dependencies": {
    "ajax-request": "^1.2.3",
    "async": "^3.2.3",
    "body-parser": "^1.19.2",
    "bootstrap": "^5.1.3",
    "express": "^4.17.3",
    "google-books-search": "^0.3.1",
    "jshint": "^2.13.4",
    "jslint": "^0.12.1",
    "node-isbn": "^1.6.1",
    "sqlite3": "^5.0.2"
