{
    "name": "Booklist",
    "description": "Booklist from rebuy",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://www.rebuy.de/verkaufen/bulk-isbn"    
    ]
}